{
  "private": true,
  "type": "module",
  "name": "watch-duty-manager",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix vite:build",
    "commit": "cz",
    "dev": "node -r dotenv/config node_modules/.bin/remix vite:dev --host 0.0.0.0",
    "db:setup": "npx prisma db push && npx prisma db seed",
    "format": "prettier --write \"./app/**/*.{ts,tsx,js,json}\"",
    "lint": "eslint \"app/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"app/**/*.{ts,tsx}\" --fix",
    "start": "remix-serve ./build/server/index.js",
    "test": "vitest run",
    "test:tsc": "tsc --noEmit --incremental",
    "test:tsc:watch": "tsc --noEmit --watch --incremental"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@remix-run/node": "^2.17.1",
    "@remix-run/react": "^2.17.1",
    "@remix-run/serve": "^2.17.1",
    "@tailwindcss/vite": "^4.1.16",
    "dotenv": "^17.2.3",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "fp-ts": "^2.16.11",
    "isbot": "^5.1.31",
    "prisma": "^6.18.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-multi-date-picker": "^4.5.2",
    "react-use": "^17.6.0",
    "recharts": "^2.15.3",
    "temporal-polyfill": "^0.3.0",
    "url-from": "^1.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.17.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "lint-staged": "^16.2.6",
    "prettier": "3.6.2",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.4.1",
    "vite-plugin-cjs-interop": "^2.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.3"
  },
  "engines": {
    "node": ">=22"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prisma": {
    "seed": "node --experimental-strip-types prisma/seed.ts"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "sideEffects": false,
  "volta": {
    "node": "22.12.0",
    "npm": "11.4.2"
  }
}
