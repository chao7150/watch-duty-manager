{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "commit": "cz",
    "dev": "node -r dotenv/config node_modules/.bin/remix dev --manual",
    "format": "prettier --write \"./app/**/*.{ts,tsx,js,json}\"",
    "lint": "eslint \"app/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"app/**/*.{ts,tsx}\" --fix",
    "start": "remix-serve ./build/index.js",
    "test": "vitest run",
    "test:tsc": "tsc -b",
    "test:tsc:watch": "tsc -b --watch"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@remix-run/node": "2.15.2",
    "@remix-run/react": "2.15.2",
    "@remix-run/serve": "2.15.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.7",
    "firebase": "^9.15.0",
    "firebase-admin": "^11.4.1",
    "firebaseui": "^6.0.2",
    "fp-ts": "^2.16.9",
    "isbot": "^5.1.19",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "react-multi-date-picker": "^4.5.2",
    "react-use": "^17.6.0",
    "recharts": "^2.15.0",
    "url-from": "^1.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "2.15.2",
    "@remix-run/eslint-config": "^2.15.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.6.0",
    "lint-staged": "^15.3.0",
    "prettier": "3.4.2",
    "prisma": "^6.1.0",
    "prisma-dbml-generator": "^0.12.0",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "sideEffects": false,
  "volta": {
    "node": "22.12.0"
  }
}
